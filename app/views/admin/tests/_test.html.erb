<tr>
  <td><%= test.title %></td>
  <td><%= test.level %></td>
  <td><%= test.category.title %></td>
  <td><%= test.author.last_name %></td>
  <td><%= test.questions.size %></td>
  <td><%= link_to t('.show'), admin_test_path(test) %></td>
  <td><%= link_to t('.edit'), edit_admin_test_path(test) %></td>
  <td><%= link_to t('.delete'), admin_test_path(test), method: :delete,
            data: { confirm: t('.delete_confirmation')} %></td>
</tr>
